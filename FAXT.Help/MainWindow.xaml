<Window x:Class="FAXT.Help.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FAXT.Help"
        mc:Ignorable="d"
        Title="Help" Height="225" Width="400" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Margin="56,16,50,28">For any questions or feedback, please visit:</TextBlock>
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,31,0,0">           
            <Hyperlink NavigateUri="https://github.com/Dacheng-Wang/FAXT" RequestNavigate="Hyperlink_RequestNavigate">
                https://github.com/Dacheng-Wang/FAXT
            </Hyperlink>
        </TextBlock>
        <Image Height="50" Margin="140,-53,141,63" VerticalAlignment="Center" Source="GitHub_Logo.png" HorizontalAlignment="Center" RenderOptions.BitmapScalingMode="HighQuality"/>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Notes: When this window is opened, the computer will stay active. Your skype status will remain online :)" VerticalAlignment="Center" Margin="22,87,20,-67" FontSize="7"/>
    </Grid>
</Window>
